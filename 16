#include <bits/stdc++.h>
using namespace std;
vector<vector<int> > graph;
int con[1][6];
void dfs(int start,int start1,vector<bool>& visited)
{
    //cout<<start1;
    visited[start]=true;
    for(int i=0;i<graph[start1].size();i++)
    {
        if(graph[start1][i]==1&&visited[i]==false)
        {
            if(con[start][start1]==1||start==start1)
                con[start][i]=1;
            dfs(start,i,visited);
        }
    }
}
int main()
{
   int n;
   cout<<"enter number of vertices"<<endl;
   cin>>n;
   int m;
   cout<<"enter number of edges"<<endl;
   cin>>m;
   graph = vector<vector<int> >(n, vector<int>(n, 0));
   cout<<"enter connection edges"<<endl;
    for(int i=0;i<m;i++)
    {
    int x,y;
    cin>>x>>y;
    graph[x][y] = 1;
    graph[y][x] = 1;
    }
    vector<bool> visited(n,false) ;
    dfs(0,4,visited);
    if(con[0][4]==1)
     cout<<"yes";
    else
     cout<<"no";
    return 0;
}
